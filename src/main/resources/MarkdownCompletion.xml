      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="Image" value="![$description$]($image_location$)" description="" toReformat="true" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
