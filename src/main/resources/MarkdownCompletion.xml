      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="Italic" value="*$string$*" description="" toReformat="true" toShortenFQNames="true">
    <variable name="string" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="Blockquotes" value="&gt; $string$" toReformat="true" toShortenFQNames="true">
