    </context>
  </template>
  <template name="URL" value="&lt;$Description$&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="Description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="Table" value="| $Description$ | | | &#10;* |-----|-----|-----| &#10;* | | | |" description="" toReformat="true" toShortenFQNames="true">
    <variable name="Description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
