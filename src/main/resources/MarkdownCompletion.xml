<templateSet group="MarkdownCompletion">
  <template name="Bold" value="**$string$**" toReformat="true" toShortenFQNames="true">
    <variable name="string" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="Heading1" value="# $string$" toReformat="true" toShortenFQNames="true">
